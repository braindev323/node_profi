                    <li class="nav-item">
                        <a class="nav-link" href="/setting">
                            <i class="fa fa-cog menu-icon"></i>
                            <span class="menu-title">Setting</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/logout">
                            <i class="fa fa-sign-out-alt menu-icon"></i>
                            <span class="menu-title">Logout</span>
                        </a>
