<% layout('app') -%>
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-12 grid-margin">
                <div class="d-flex align-items-baseline flex-wrap mt-3">
                    <h2 class="mr-4 mb-0">Users</h2>
                    <div class="d-flex align-items-baseline mt-2 mt-sm-0">
                        <i class="fas fa-user mr-1 text-muted"></i>
                        <i class="fas fa-chevron-right fa-xs mr-1 text-muted"></i>
                        <p class="mb-0 mr-1"><a href="/users">Users</a></p>
                        <i class="fas fa-chevron-right fa-xs mr-1 text-muted"></i>
                        <p class="mb-0 mr-1">User Detail</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="card">
            <div class="card-body">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive table-bordered">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Tm Evnets</th>
                                        <th>Sh Events</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><%=tm_events.length%></td>
                                        <td><%=sh_events.length%></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="row mt-5">
                    <div class="col-12 col-md-6">
                        <h4 class="mb-4">Stored ticketmaster eventIDs</h4>
                        <div class="table-resonsive">
                            <table class="table order-listing">
                                <thead>
                                    <th>No</th>
                                    <th>EventID</th>
                                </thead>
                                <tbody>
                                    <% for(let i = 0; i < tm_events.length; i++) { %>
                                    <tr>
                                        <td><%=i+1%></td>
                                        <td><%=tm_events[i]%></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-12 col-md-6">
                        <h4 class="mb-4">Stored stubhub eventIDs</h4>
                        <div class="table-resonsive">
                            <table class="table order-listing">
                                <thead>
                                    <th>No</th>
                                    <th>EventID</th>
                                </thead>
                                <tbody>
                                    <% for(let i = 0; i < sh_events.length; i++) { %>
                                    <tr>
                                        <td><%=i+1%></td>
                                        <td><%=sh_events[i]%></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>